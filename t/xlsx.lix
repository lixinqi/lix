#!/usr/local/bin/node ./lix.js
excel := 'excel' require
JSON := 'JSON' require

./2014-gaokao.xlsx excel (err data)->{
	head := data.0 .slice 1 -1
	body := data .slice 1 -1

	ret := {}
	head foreach (item index)->{
		ret.[item] = {}
		body foreach (province)->{
			provinceName := province.0
			ret.[item].[provinceName] = province.[(index parseInt) + 1] * 10000
		}
	}
	ret | JSON.stringify null 4 | print
}

