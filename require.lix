#!/usr/local/bin/node ./lix.js
module := require
jq := 'jquery' module | call ('jsdom' module | .jsdom | .createWindow)

html := "<html><title>nice</title><p id='id' data-yes='good'>fine</p></html>"

html | jq | .find 'title' | .text | print

html | jq | .find '#id' | .text | print

html | jq | .find '#id' | .data 'yes' | print

html | jq | .find '#id' | .data | .yes | print

