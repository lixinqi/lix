#!/usr/local/bin/node

[xl end]->{
	cont := null
	return := null
	cc call [break]->{
		return = break
		yieldOne := [i]->{
			cc call [next]->{
				cont = next
				i return
			}
		}
		0 yieldOne
		xl | map yieldOne | exec
		cont = identity
		end return
	}
	[]->{
		cc call [break]->{
			return = break
			cont call
			end return
		}
	}
}
