# $objectOriented print

Base := {}

MyObject := Base $objectOriented.subclass [pub]->{
	pub.__init__ = [self name]->{
		self.name = name
	}
}

_Object := Base $objectOriented.subclass

obj := MyObject $objectOriented.new 'Bob'
obj print

MyObject is Base | print
obj instanceof MyObject | print
obj is MyObject | print
obj is Base | print

MyObject is _Object | print
obj instanceof _Object | print
obj is _Object | print
obj is _Object | print
