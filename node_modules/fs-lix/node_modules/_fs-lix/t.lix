'$fs.ReadFileAsync' $test [assert]->{
	/etc/passwd $fs.ReadFileAsync 'utf-8' [err data]->{
		(err == null) assert
		(data != null) assert
	}

	/etc/passwd $fs.ReadFileAsync [err data]->{
		(err == null) assert
		(data != null) assert
	}

	/this-is-not-dir/1p29384-this-is-not-a-file $fs.ReadFileAsync [err data]->{
		(err != null) assert
		(data == null) assert
	}
}
