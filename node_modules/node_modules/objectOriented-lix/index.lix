subclass := [base config]->{
	class := base Object.Create
	if (config) {
		class config
	}
	class
}

new := [class]->{
	obj := class Object.Create
	args := arguments Array.Slice
	args.0 = obj
	if (class.__init__) {
		class.__init___ apply args
	}
	obj
}

{
	.subclass subclass
	.new new
	.instanceof instanceof
	.is is
}
