#!/usr/bin/env node

$try call []->{
	'good' print
} [e]->{
}

'nice' print

$try call []->{
	'try begin' print
	'Error' $raise
	'try end' print
} [e]->{
	'Catch' print
	e print
}

$try call []->{
	$try call []->{
		'Inner error in try block' $raise
		'!!!!!!!!!!!!!!!! impossable to run here' print
	} [e]->{
		e print
		'Outer error in catch block' $raise
	}
	'!!!!!!!!!!!!!!!! impossable to run here' print
} [e]->{
	e print
}

'end' print
