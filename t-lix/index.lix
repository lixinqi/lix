#!/usr/local/bin/node

pair fn (x: Number) (y: Number) = [x y]
pair fn [(x: Number) (y: Number)] = [x y]
pair fn {.fst (x: Number) .snd (y: Number)} = [x y]

pair call 3 4 | print
pair call [3 4] | print
pair call {.fst 3 .snd 4} | print

f fn {.data [(x: Number) (y: Number)]} = [x y]
f call {.data [3 4]} | print

p fn (x: Number) (y: Number) ... (z: Number) = [x y z]

p call 3 4 5 | print
